package day04;
import java.util.Scanner;

public class Problem03 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int num = 0, i = 0;
		int[] arr = new int[5];
		
		while(i < 5) {
			num = sc.nextInt();
			if(num % 2 == 0) {
				arr[i] = num;
				i++;
			}
		}
	}
}
