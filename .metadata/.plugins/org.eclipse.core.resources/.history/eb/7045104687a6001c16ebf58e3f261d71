package day0315;

import java.util.Scanner;

public class BJ {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// 한 줄의 단어는 글자들을 빈칸 없이 최대 15개를 연속으로 받음 -> sc.nextLine()
		// 세로로 읽고 -> 행 바뀜, 열 그대로
		// 순서대로 글자들을 공백 없이 출력 -> stringbuilder로 묶어서 출력
		// 공백도 받음, 공백은 읽지 않음

		String str = "";
		char[][] strArr = new char[5][];

		for (int row = 0; row < 5; row++) {
			strArr[row].

					str = sc.nextLine();
			for (int col = 0; col < str.length(); col++)
				strArr[row][col] = str.charAt(col);
		}

		StringBuilder sb = new StringBuilder();
		for (int j = 0; j < 5; j++) {
			char res;
			for (int i = 0; i < 5; i++) {
				res = strArr[i][j];
				if (res != ' ')
					sb.append(res);
				else
					continue;
			}
		}

		System.out.println(sb);

	}
}