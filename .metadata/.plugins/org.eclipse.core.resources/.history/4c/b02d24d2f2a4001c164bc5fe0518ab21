package kr.ac.kopo.day06;

public class Gugudan {
	void print(int dan) {
		System.out.println("*** " + dan + "´Ü ***");
		for (int i = 1; i <= 9; i++) {
			System.out.printf("%d * %d = %d\n", dan, i, (dan * i));
		}
	}
	void print() {
		for(int dan = 2; dan <= 9; dan++) {
			System.out.println("*** " + dan + "´Ü ***");
			for(int i = 1; i <= 9; i++) {
				System.out.printf("%d * %d = %d\n", dan, i, (dan * i));
			}
		}
	}
}
