package kr.ac.kopo.day06;

/*
**********
Hello
**********
Hi
**********
Bye
**********
*/

public class MethodMain01 {
	static void printStar() {
		for (int i = 0; i < 10; i++) {
			System.out.print("*");
		}
	}

	public static void main(String[] args) {
		printStar();
		System.out.println("Hello");
	}
}
