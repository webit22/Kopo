package day0315;

import java.util.Scanner;

public class BJ {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);

		// 한 줄의 단어는 글자들을 빈칸 없이 최대 15개를 연속으로 나열
		// 세로로 읽은 순서대로 글자들을 공백 없이 출력 .char(i)
		// 공백도 받음, 공백은 읽지 않음
		/*
		 * 1. 입력받음 2. 배열에 저장 3. 간격 5씩
		 */
		String str = "";
		char[][] strArr = new char[5][];
		for (int row = 0; row < 5; row++) {
			str = sc.nextLine();

			for (int col = 0; col < str.length(); col++)
				strArr[row][col] = str.charAt(col);
		}

		int strLen = str.length();
		char res;
		for (int j = 0; j < 5; j++) {
			for (int i = 0; i < 5; i++) {
				res = strArr[i][j];
				if (res != ' ')
					System.out.println(res);
			}
		}

	}
}